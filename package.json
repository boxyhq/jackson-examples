{
  "name": "jackson-examples",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "_install:adonisjs": "cd apps/adonisjs && npm i",
    "_install:auth0-user-store": "cd apps/auth0-user-store && npm i",
    "_install:directory-sync": "cd apps/directory-sync && npm i",
    "_install:directory-sync-embedded": "cd apps/directory-sync-embedded && npm i",
    "_install:express": "cd apps/express && npm i",
    "_install:express-api": "cd apps/express-api && npm i",
    "_install:firebase-user-store": "cd apps/firebase-user-store && npm i",
    "_install:hasura-express": "cd apps/hasura-express && npm i",
    "_install:hasura-nextjs": "cd apps/hasura-nextjs && npm i",
    "_install:next-auth": "cd apps/next-auth && npm i",
    "_install:react": "cd apps/react && npm i --force",
    "install": "run-p '_install:*'",
    "dev:adonisjs": "cd apps/adonisjs && npm run dev",
    "dev:auth0-user-store": "cd apps/auth0-user-store && npm run dev",
    "dev:directory-sync": "cd apps/directory-sync && npm run dev",
    "dev:directory-sync-embedded": "cd apps/directory-sync-embedded && npm run dev",
    "dev:express": "cd apps/express && npm run dev --filter='express'",
    "dev:express-api": "cd apps/express-api && npm run start",
    "dev:firebase-user-store": "cd apps/firebase-user-store && npm run dev",
    "dev:hasura-express": "cd apps/hasura-express && npm run dev",
    "dev:hasura-nextjs": "cd apps/hasura-nextjs && npm run dev",
    "dev:next-auth": "cd apps/next-auth && npm run dev",
    "dev:react": "npm run dev:express-api & cd apps/react && npm run start"
  },
  "engines": {
    "node": ">=14.18.1 <=18.x"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}
