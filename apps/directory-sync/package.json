{
  "name": "directory-sync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "dev": "prisma db push && prisma db seed && next dev -p 3366",
    "build": "next build",
    "start": "next start -p 3366",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "4.6.1",
    "classnames": "^2.3.1",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@types/node": "18.8.5",
    "@types/react": "18.0.21",
    "autoprefixer": "^10.4.2",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "postcss": "^8.4.8",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.8.0",
    "typescript": "4.9.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
