{
  "name": "directory-sync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "dev": "prisma db push && prisma db seed && next dev -p 3366",
    "build": "next build",
    "start": "next start -p 3366",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "4.7.0",
    "classnames": "2.3.2",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.3",
    "@types/node": "18.11.10",
    "@types/react": "18.0.25",
    "autoprefixer": "10.4.13",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "postcss": "8.4.19",
    "prisma": "4.7.1",
    "tailwindcss": "3.2.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
