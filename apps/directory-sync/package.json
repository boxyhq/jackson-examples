{
  "name": "directory-sync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "dev": "prisma db push && prisma db seed && next dev -p 3366",
    "build": "next build",
    "start": "next start -p 3366",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "5.1.0",
    "classnames": "2.3.2",
    "next": "13.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.6",
    "@types/node": "20.3.3",
    "@types/react": "18.2.18",
    "autoprefixer": "10.4.14",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.12",
    "postcss": "8.4.24",
    "prisma": "5.1.0",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
