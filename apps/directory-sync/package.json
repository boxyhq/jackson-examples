{
  "name": "directory-sync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "dev": "prisma db push && prisma db seed && next dev -p 3366",
    "build": "next build",
    "start": "next start -p 3366",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "classnames": "2.3.2",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.7",
    "@types/node": "20.8.2",
    "@types/react": "18.2.39",
    "autoprefixer": "10.4.16",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.4",
    "postcss": "8.4.31",
    "prisma": "5.7.0",
    "tailwindcss": "3.3.6",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
